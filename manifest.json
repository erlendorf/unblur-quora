{
  "manifest_version": 2,
  "name": "UnBlur Quora",
  "description": "Read unlimited Quora answers without signing up.",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
